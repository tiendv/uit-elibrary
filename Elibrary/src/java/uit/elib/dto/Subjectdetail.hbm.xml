<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="uit.elib.dto.Subjectdetail" table="subjectdetail">
    <composite-id>
        <key-many-to-one name="faculty" class="uit.elib.dto.Faculty" column="FacultyID"/>
        <key-many-to-one name="subject" class="uit.elib.dto.Subject" column="SubjectID"/>
    </composite-id>
  </class> 
</hibernate-mapping>
